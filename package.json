{
  "name": "@devoxa/mock-oidc",
  "description": "Mock OpenID Connect sign-in for test environments",
  "version": "1.0.0",
  "main": "dist/index.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "url": "https://github.com/devoxa/mock-oidc"
  },
  "scripts": {
    "start": "ts-node-dev --exit-child --respawn --rs src/index.ts",
    "format": "prettier --ignore-path='.gitignore' --list-different --write .",
    "format:check": "prettier --ignore-path='.gitignore' --check .",
    "lint": "eslint 'src/**/*.ts'",
    "build": "rm -rf dist/ && tsc",
    "preversion": "yarn build"
  },
  "prettier": "@devoxa/prettier-config",
  "dependencies": {
    "body-parser": "1.20.4",
    "express": "4.22.1",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@devoxa/eslint-config": "4.0.2",
    "@devoxa/prettier-config": "2.0.3",
    "@types/express": "4.17.25",
    "@types/node": "20.9.5",
    "eslint": "9.39.2",
    "prettier": "3.8.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.9.3"
  },
  "volta": {
    "node": "20.9.0"
  }
}
